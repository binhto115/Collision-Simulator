{
    "name": "collision-simulator",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "start": "npm run start:backend",
        "dev": "npm -w packages/react-frontend run dev",
        "dev:backend": "npm -w packages/express-backend run dev",
        "start:backend": "npm -w packages/express-backend run start",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "npm run -w express-backend lint && npm run -w react-frontend lint",
        "lint:all": "npm run lint",
        "build": "npm run -w react-frontend build"
    },
    "license": "ISC",
    "devDependencies": {
        "prettier": "^3.6.2"
    },
    "dependencies": {
        "bcrypt": "^6.0.0",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2"
    },
    "description": "## Contributing",
    "main": "index.js",
    "keywords": [],
    "author": ""
}